<div class="page-header">
  <h3>Hostphoto #<%= @hostphoto.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Photo</dt>
      <dd>
        <% if @hostphoto.photo.present? %>
          <a href="/photos/<%= @hostphoto.photo_id %>">
            <%= @hostphoto.photo.created_at %>
          </a>
        <% end %>
      </dd>

      <dt>Listing</dt>
      <dd>
        <% if @hostphoto.listing.present? %>
          <a href="/listings/<%= @hostphoto.listing_id %>">
            <%= @hostphoto.listing.title %>
          </a>
        <% end %>
      </dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/hostphotos" class="btn btn-primary">
        Back
      </a>
      <a href="/hostphotos/<%= @hostphoto.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_hostphoto/<%= @hostphoto.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
